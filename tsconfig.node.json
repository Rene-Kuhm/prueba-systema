{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./",
    "composite": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"]
  },
  "include": ["vite.config.ts"],
  "exclude": ["node_modules"]
}