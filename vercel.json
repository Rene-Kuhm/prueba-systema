{
    "builds": [
    { "src": "package.json", "use": "@vercel/node" }
  ],
    "routes": [
        {
        
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        },
        { "src": "/sw.js", "dest": "/sw.js" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}